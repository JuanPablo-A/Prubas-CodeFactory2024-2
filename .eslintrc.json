{
  "extends": ["next/core-web-vitals", "next/typescript"],
    "rules": {
      "camelcase": ["warn", { "properties": "never", "ignoreDestructuring": false }],
      "id-match": [
        "warn",
        "^[a-z][a-zA-Z0-9]*$|^[A-Z][a-zA-Z0-9]*$|^[A-Z0-9_]+$",
        {
          "onlyDeclarations": true,
          "properties": false
        }
      ],
      "eqeqeq": ["error", "always"]
    }
}
